{
  "mcpServers": {
    "postgres": {
      "command": "bunx",
      "args": ["@carlos.e.alves3/mcp-postgres-server"],
      "env": {
        "POSTGRES_HOST": "localhost",
        "POSTGRES_PORT": "5432",
        "POSTGRES_DATABASE": "mydb",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "your_password_here",
        "POSTGRES_MAX_CONNECTIONS": "10",
        "POSTGRES_INSECURE": "false"
      }
    }
  }
}
